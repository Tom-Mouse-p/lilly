function convertJSONToLowerCase(obj) {
    if (typeof obj === "string") {
      return obj.toLowerCase();
    } else if (typeof obj === "object") {
      const result = {};
      for (let key in obj) {
        if (obj.hasOwnProperty(key)) {
          const newKey = key.toLowerCase();
          result[newKey] = convertJSONToLowerCase(obj[key]);
        }
      }
      return result;
    } else if (Array.isArray(obj)) {
      return obj.map(item => convertJSONToLowerCase(item));
    } else {
      return obj;
    }
  }
  
  const originalJSON = {
    "open": {
      "Open Google": "https://www.google.com",
      "Open YouTube": "https://www.youtube.com",
      "Open Facebook": "https://www.facebook.com"
    },
    "search": {
      "YouTube": "https://www.youtube.com/results?search_query=oaremon"
    }
  };
  
  const lowercaseJSON = convertJSONToLowerCase(originalJSON);
  
  console.log(JSON.stringify(lowercaseJSON, null, 2));
  